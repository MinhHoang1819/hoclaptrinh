#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main(){
    ll S = 0;
    vector<int> arr;
    while (true){
        int x; cin >> x;
        if (x == -1) break;
        arr.push_back(x);
    }
    for (int i=0; i<arr.size(); i++)
        S += arr[i];
    cout << S;
    return 0;
}
