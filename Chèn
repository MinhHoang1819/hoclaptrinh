#include <iostream>
#include <bits/stdc++.h>

using namespace std;
struct node{
	int nd;
	node *link;
};
node *first = NULL;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
void chenDau(int x, node *&first){
	node *p = new node;
	p->nd = x;
	p->link = first;
	first = p;
}
void inan(node *first){
	if (first != NULL){
		cout << first->nd << "\t";
		inan(first->link);
	}
}
int main(int argc, char** argv) {
	int n; cin >> n;
	for (int i=1; i<=n; i++){
		int x; cin >> x;
		if (x%2 == 0){
			chenDau(x, first);
		}
	}
	/*chenDau(5, first);
	chenDau(7, first);
	chenDau(9, first);*/
	inan(first);
	return 0;
}
