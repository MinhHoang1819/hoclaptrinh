#include<bits/stdc++.h>
using namespace std;
int main()
{
    cout << fixed << setprecision(6);
    long long n; cin >> n;
    long long tmp = n;
    long long r = 0;
    while (n!=0){
        r= r*10 + n%10;
        n = n/10;
    }
    if (r==tmp)
        cout << "yes";
    else
        cout << "no";
    return 0;
}
