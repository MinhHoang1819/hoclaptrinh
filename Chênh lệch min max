#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;
    int arr[n+1]; //khai báo động giống như con trỏ trong C
    for (int i=1; i<=n; i++)
        cin >> arr[i];
    int max = arr[0], min = arr[0];
    for (int i=1; i<n; i++)
        if (arr[i] > max) max = arr[i];
    for (int i=1; i<n; i++)
        if (arr[i] < min) min = arr[i];
    cout << max-min;
    return 0;
}
